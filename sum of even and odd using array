#include <stdio.h>
int main()
{
    int n,esum,osum;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0,esum=0,osum=0;i<n;i++){
        if(a[i]%2!=0){
            osum=osum+a[i];
        }
    else{
        esum=esum+a[i];
    }    
}
printf("%d\n",osum);
    printf("%d",esum);
}

